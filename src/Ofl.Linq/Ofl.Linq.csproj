<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard1.0</TargetFramework>
    <Description>Extensions for working with enumerables in .NET, a-la LINQ.</Description>
    <AssemblyName>Ofl.Linq</AssemblyName>
    <PackageId>Ofl.Linq</PackageId>
    <PackageProjectUrl>https://github.com/OneFrameLink/Ofl.Linq</PackageProjectUrl>
    <RepositoryUrl>https://github.com/OneFrameLink/Ofl.Linq.git</RepositoryUrl>
    <Version>1.2.0</Version>
    <PackageTags>linq enumerable</PackageTags>

    <!-- Declare that the Repository URL can be published to NuSpec -->
    <PublishRepositoryUrl>true</PublishRepositoryUrl>    
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0-beta-62925-02" PrivateAssets="All" />
  </ItemGroup>
</Project>